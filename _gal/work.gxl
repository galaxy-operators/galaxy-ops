extern mod mod_ops { git =  "https://github.com/galaxy-operators/ops-gxl.git", channel = "${GXL_CHANNEL:beta}" }
mod envs : {
    env  _common {
        module_env = "default";
    }
    env arm_mac : _common {
        model = "arm-mac14-host";
    }
    env x86_ubt : _common {
        model = "x86-ubt22-host";
    }

}
mod main :  mod_ops {
    flow conf {
         gx.cmd("gops mod update")   ;
         gx.cmd("gops mod localize")   ;
    }
}
